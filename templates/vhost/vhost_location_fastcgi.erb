  location <% if @match_type %><%= match_type %><% end -%> <%= location %> {
    fastcgi_pass <%= upstream %>;

    <% fastcgi_params.sort.map do |param,value|  -%>
    fastcgi_param <%= param %> <%= value %>;
    <% end -%>


    <% other_directives.sort.map do |directive,value|  -%>
    <%= directive %> <%= value %>;
    <% end -%>

  }
